{
  "name": "api.dash.acaddio.com",
  "version": "0.0.1",
  "description": "Student Information System",
  "main": "index.js",
  "scripts": {
    "es-migrate": "node elasticsearch/migrate.js",
    "es-migrate-undo": "node elasticsearch/migrate-undo.js",
    "start": "node --max_old_space_size=800 ./bin/www",
    "test": "npm run cleanup && mocha test/setup.js test/api/**/*.test.js",
    "test-nocleanup": "mocha test/setup.js test/api/**/*.test.js",
    "cover": "npm run cleanup && istanbul cover _mocha test/setup.js test/api/**/*.test.js",
    "cover-nocleanup": "istanbul cover _mocha test/setup.js test/api/**/*.test.js",
    "startdev": "DEBUG=api nodemon ./bin/www",
    "migrate": "sequelize db:migrate",
    "migrate-undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed-undo": "sequelize db:seed:undo:all",
    "cleanup": "npm run migrate-undo && npm run migrate && npm run seed && npm run es-migrate-undo && npm run es-migrate"
  },
  "keywords": [
    "acaddio",
    "sis",
    "information",
    "system",
    "student"
  ],
  "author": "Lucas Lima",
  "devDependencies": {
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.1",
    "mocha": "^2.3.3",
    "nodemon": "^1.7.3",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "@risingstack/trace": "^3.0.2",
    "app-module-path": "^1.0.4",
    "app-root-path": "^1.2.1",
    "aws-sdk": "^2.7.18",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.0.3",
    "body-parser": "^1.14.1",
    "canvas": "^1.6.2",
    "color-thief": "^2.2.2",
    "commander": "^2.9.0",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "cors": "^2.8.1",
    "crypto-js": "^3.1.7",
    "debug": "^2.2.0",
    "diacritics": "^1.2.3",
    "elasticsearch": "^12.1.3",
    "escape-html": "^1.0.3",
    "express": "^4.13.3",
    "express-pino-logger": "^2.0.0",
    "file-type": "^4.0.0",
    "fs-readdir-recursive": "^1.0.0",
    "hex-rgb": "^1.0.0",
    "html-minifier": "^3.2.3",
    "i": "^0.3.5",
    "jimp": "^0.2.27",
    "json-refs": "^2.1.6",
    "jsonwebtoken": "^5.4.0",
    "lodash": "^4.3.0",
    "memwatch-next": "^0.2.9",
    "mime": "^1.3.4",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "multer": "^1.1.0",
    "nconf": "^0.8.4",
    "normalize-url": "^1.8.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "pg-native": "^1.10.0",
    "pino": "^3.0.7",
    "pngjs": "^3.0.0",
    "pug": "^2.0.0-beta3",
    "qr-image": "^3.2.0",
    "redis": "^2.3.0",
    "rgb-hex": "^2.1.0",
    "sequelize": "^3.27.0",
    "sequelize-cli": "^2.3.1",
    "serve-favicon": "^2.3.2",
    "uuid": "^2.0.1",
    "validator": "^4.3.0",
    "yaml-js": "^0.1.4"
  }
}
